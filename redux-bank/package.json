{
  "name": "redux-bank",
  "description": "A banking app with React/Redux",
  "author": "William Horton",
  "license": "MIT",
  "scripts": {
    "start": "node server.js",
    "build": "webpack --config ./webpack.production.config.js --progress --colors",
    "test": "NODE_ENV=test mocha --recursive --compilers js:babel-core/register",
    "test:watch": "npm test -- --watch",
    "container:build": "docker build -t react-docker .",
    "container:start": "docker run --name react-docker -p 80:8088 react-docker",
    "container:stop": "docker stop react-docker && docker rm react-docker"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "babel-core": "^6.3.17",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "express": "^4.17.3",
    "history": "^1.13.0",
    "react": "^0.14.0",
    "react-dom": "^0.14.0",
    "react-redux": "^4.0.0",
    "react-router": "^1.0.2",
    "redux": "^3.0.0",
    "redux-form": "^4.0.5",
    "redux-simple-router": "^1.0.1",
    "redux-thunk": "^2.2.0",
    "semantic-ui-css": "^2.1.6",
    "webpack": "^1.9.11"
  },
  "devDependencies": {
    "expect": "^1.13.4",
    "install": "^0.4.1",
    "mocha": "^2.2.5",
    "react-addons-test-utils": "^0.14.0",
    "webpack-dev-middleware": "^1.4.0"
  },
  "engines": {
    "node": ">=8.0.0 <11.0.0"
  }
}
